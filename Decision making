    print('no')

#budget
a=float(input())
b=float(input())
c=float(input())
d=float(input())
e=float(input())
s=(a*350.34+b*230.90+c*190.55+d*125.30+e*180.90)
if(s<+15000):
    print('yes')
else:
    print('no')
    
    #sece dining
    a=str(input())
b=int(input())
if(a=='front' and b==1):
    print('Left handed')
elif(a=='front' and b==2):
    print('Right handed')
elif(a=='rear' and  b==1):
    print('Right handed')
elif(a=='rear' and b==2):
    print('Left handed')
else:
    print('invalid input')
    
    #intersection of a circle
    import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
a=math.sqrt((x1-x2)**2+(y1-y2)**2)
print(a)
if r1+r2==a:
    print("the circles are tangential")
elif r1+r2>a:
    print("the circles overlap")
else:
    print("the circles overlap")
    
    #micro oven
    a=int(input('enter the number of items\n'))
b=float(input('enter the single item heating time\n'))
if(a==1):
    print('b')
elif(a==2):
    print('the recommended heating time is ',"%.2f"%(1.5*b))
elif(a==3):
    print(2*b)
else:
    print("number of items is more")
    
