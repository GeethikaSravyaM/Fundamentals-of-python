#Amoeba Multiplication
n=int(input("Enter the number of months:"))
a,b=0,1
count=3
while(count<=n):
    c=a+b
    a=b
    b=c
    count=count+1
print("The amoeba size is ",b)

#Factorial
n=int(input())
factorial=1
i=2
while(True):
    factorial=factorial*i
    i=i+1
    if(factorial==n):
        print("yes")
        break
    if(factorial>n):
        print("no")
        break
        
#Sum of digits
n=input()
sum=0
for i in n:
    sum=sum+int(i)
print(sum)
